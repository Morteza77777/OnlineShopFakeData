<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/back_app"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view11"
        android:layout_width="0dp"
        android:layout_height="220dp"
        android:background="@drawable/back_view_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/userProfile"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view11" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_profile1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="8dp"
        android:src="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/view11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView24" />

    <ImageView
        android:id="@+id/img_profile"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:scaleType="fitXY"
        android:src="@drawable/img_40"
        app:layout_constraintBottom_toBottomOf="@+id/img_profile1"
        app:layout_constraintEnd_toEndOf="@+id/img_profile1"
        app:layout_constraintStart_toStartOf="@+id/img_profile1"
        app:layout_constraintTop_toTopOf="@+id/img_profile1" />

    <TextView
        android:id="@+id/txt_name_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        tools:text="بهرام افشاری "
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="@+id/img_profile1"
        app:layout_constraintStart_toStartOf="@+id/img_profile1"
        app:layout_constraintTop_toBottomOf="@+id/img_profile1" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        tools:text="09024489809"
        android:textSize="15sp"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/txt_name_profile"
        app:layout_constraintStart_toStartOf="@+id/txt_name_profile"
        app:layout_constraintTop_toBottomOf="@+id/txt_name_profile" />

    <ImageButton
        android:id="@+id/img_edit"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="78dp"
        android:background="@drawable/back_image_button_profile"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toStartOf="@+id/img_profile1"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_41" />

    <ImageButton
        android:id="@+id/img_edit_profile"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/back_image_button_profile"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toEndOf="@+id/img_profile1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_42" />

    <View
        android:id="@+id/view_orders"
        android:layout_width="145dp"
        android:layout_height="135dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/back_edt_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view11" />

    <View
        android:id="@+id/view14"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/back_image_view_product_profile"
        app:layout_constraintEnd_toEndOf="@+id/view_orders"
        app:layout_constraintStart_toStartOf="@+id/view_orders"
        app:layout_constraintTop_toTopOf="@+id/view_orders" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/view14"
        app:layout_constraintEnd_toEndOf="@+id/view14"
        app:layout_constraintStart_toStartOf="@+id/view14"
        app:layout_constraintTop_toTopOf="@+id/view14"
        app:srcCompat="@drawable/img_43" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/myOrders"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/view_orders"
        app:layout_constraintStart_toStartOf="@+id/view_orders"
        app:layout_constraintTop_toBottomOf="@+id/view14" />

    <View
        android:id="@+id/view_experience"
        android:layout_width="145dp"
        android:layout_height="135dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/back_edt_text"
        app:layout_constraintEnd_toStartOf="@+id/view_orders"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view_orders" />

    <View
        android:id="@+id/view1"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/back_image_view_product_profile"
        app:layout_constraintEnd_toEndOf="@+id/view_experience"
        app:layout_constraintStart_toStartOf="@+id/view_experience"
        app:layout_constraintTop_toTopOf="@+id/view_experience" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/view1"
        app:layout_constraintEnd_toEndOf="@+id/view1"
        app:layout_constraintStart_toStartOf="@+id/view1"
        app:layout_constraintTop_toTopOf="@+id/view1"
        app:srcCompat="@drawable/img_44" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/experience"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/view_experience"
        app:layout_constraintStart_toStartOf="@+id/view_experience"
        app:layout_constraintTop_toBottomOf="@+id/view1" />

    <View
        android:id="@+id/view_password"
        android:layout_width="145dp"
        android:layout_height="135dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/back_edt_text"
        app:layout_constraintEnd_toEndOf="@+id/view_orders"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/view_orders"
        app:layout_constraintTop_toBottomOf="@+id/view_orders" />

    <View
        android:id="@+id/view2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/back_image_view_product_profile"
        app:layout_constraintEnd_toEndOf="@+id/view_password"
        app:layout_constraintStart_toStartOf="@+id/view_password"
        app:layout_constraintTop_toTopOf="@+id/view_password" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2"
        app:srcCompat="@drawable/img_45" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/changePassword"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/view_password"
        app:layout_constraintStart_toStartOf="@+id/view_password"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <View
        android:id="@+id/view_notif"
        android:layout_width="145dp"
        android:layout_height="135dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/back_edt_text"
        app:layout_constraintEnd_toStartOf="@+id/view_password"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_orders" />

    <View
        android:id="@+id/view3"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/back_image_view_product_profile"
        app:layout_constraintEnd_toEndOf="@+id/view_notif"
        app:layout_constraintStart_toStartOf="@+id/view_notif"
        app:layout_constraintTop_toTopOf="@+id/view_notif" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="@+id/view3"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toTopOf="@+id/view3"
        app:srcCompat="@drawable/img_46" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/myNotif"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/view_notif"
        app:layout_constraintStart_toStartOf="@+id/view_notif"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <View
        android:id="@+id/view_favorite"
        android:layout_width="145dp"
        android:layout_height="135dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/back_edt_text"
        app:layout_constraintEnd_toEndOf="@+id/view_password"
        app:layout_constraintStart_toStartOf="@+id/view_password"
        app:layout_constraintTop_toBottomOf="@+id/view_password" />

    <View
        android:id="@+id/view4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/back_image_view_product_profile"
        app:layout_constraintEnd_toEndOf="@+id/view_favorite"
        app:layout_constraintStart_toStartOf="@+id/view_favorite"
        app:layout_constraintTop_toTopOf="@+id/view_favorite" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/view4"
        app:layout_constraintEnd_toEndOf="@+id/view4"
        app:layout_constraintStart_toStartOf="@+id/view4"
        app:layout_constraintTop_toTopOf="@+id/view4"
        app:srcCompat="@drawable/img_47" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/myFavorite"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/view_favorite"
        app:layout_constraintStart_toStartOf="@+id/view_favorite"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <View
        android:id="@+id/view_address"
        android:layout_width="145dp"
        android:layout_height="135dp"
        android:background="@drawable/back_edt_text"
        app:layout_constraintEnd_toEndOf="@+id/view_notif"
        app:layout_constraintStart_toStartOf="@+id/view_notif"
        app:layout_constraintTop_toTopOf="@+id/view_favorite" />

    <View
        android:id="@+id/view5"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/back_image_view_product_profile"
        app:layout_constraintEnd_toEndOf="@+id/view_address"
        app:layout_constraintStart_toStartOf="@+id/view_address"
        app:layout_constraintTop_toTopOf="@+id/view_address" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:srcCompat="@drawable/img_48" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/myAddress"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/view_address"
        app:layout_constraintStart_toStartOf="@+id/view_address"
        app:layout_constraintTop_toBottomOf="@+id/view5" />


</androidx.constraintlayout.widget.ConstraintLayout>